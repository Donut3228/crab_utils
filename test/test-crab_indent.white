			fi
			[ "$my_continue" = 1 ] && continue
			conf=/node/etc/main.conf
			if grep -qm1 "^${val_name}=" "$conf"; then
				. "$conf"
				echo "${conf##*/}:${val_name}=${!val_name}"
				my_continue=1
			fi
			[ "$my_continue" = 1 ] && continue
			echo "CALC:${val_name}=${!val_name}"

		done
	) | sort

__node(){
	shift
	case $1 in
	"list")
		__node_list "$@"
		;;
	"do")
		__node_do "$@"
		;;
	"update")
		node_update "$@"
		;;
	*)
		echo "Not supported: $@"
		;;
	esac
	return 0
}

