					echo "${conf##*/}:${val_name}=${!val_name}"
					my_continue=1
				fi
			done
			[ "$my_continue" = 1 ] && continue
			conf=/node/node$NODE_ID/etc/node.conf
			if grep -qm1 "^${val_name}=" "$conf"; then
				. "$conf"
				echo "${conf##*/}:${val_name}=${!val_name}"
